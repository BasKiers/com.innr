  "flow": {
    "triggers": [
         {
             "id": "remote_button_pressed",
             "title": {
                 "en": "Remote button pressed",
                 "nl": "Remote knop gedrukt"
             },
             "tokens": [
               {
                   "name": "button_number",
                   "type": "number",
                   "title": {
                       "en": "Buttonnumber pushed",
                       "nl": "Knopnummer gedrukt"
                   },
                   "example": {
                       "en": "+"
                   }
               },
               {
                  "name": "button_type",
                  "type": "string",
                  "title": {
                      "en": "Type of button pushed",
                      "nl": "Type knop gedrukt"
                  },
                  "example": {
                      "en": "on"
                  }
                }
             ],
             "args": [
                 {
                     "name": "device",
                     "type": "device",
                     "filter": "driver_id=remote"
                 },
                 {
                   "name": "button",
                   "type": "dropdown",
                   "values": [
                     {
                       "id": "1",
                       "label": {
